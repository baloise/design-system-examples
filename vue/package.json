{
  "name": "vue-starter-kit",
  "version": "0.1.0",
  "description": "Boilerplate project to start a Vue Baloise Application",
  "license": "Apache-2.0",
  "author": {
    "name": "Gery Hirschfeld",
    "email": "gerhard.hirschfeld@baloise.ch",
    "url": "https://github.com/hirsch88"
  },
  "scripts": {
    "preeinstall": "npx npm-force-resolutions",
    "update:registry": "sed -i -e \"s#nexus.balgroupit.com/repository/npm#registry.npmjs.org#g\" package-lock.json\n",
    "postinstall": "npm run copy:fonts",
    "copy:fonts": "copyfiles --flat node_modules/@baloise/design-system-fonts/lib/* public/assets/fonts",
    "serve": "vue-cli-service serve --port 3000",
    "build": "vue-cli-service build --mode production",
    "test": "start-server-and-test serve http-get://localhost:3000 cypress:run",
    "test:ci": "start-server-and-test serve http-get://localhost:3000 cypress:run:ci",
    "lint": "vue-cli-service lint",
    "format": "npm run prettier:write",
    "prettier:write": "prettier --write \"./src\"",
    "prettier:check": "prettier --check \"./src\"",
    "cypress:run": "cypress run --config video=false,screenshotOnRunFailure=false",
    "cypress:run:ci": "cypress run --config",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@ag-grid-community/all-modules": "^26.2.0",
    "@ag-grid-community/client-side-row-model": "^26.2.0",
    "@ag-grid-community/core": "^26.2.0",
    "@ag-grid-community/vue3": "^26.2.0",
    "@baloise/design-system-components-table": "^2.0.0-beta.1",
    "@baloise/design-system-components-vue": "^2.0.0-beta.1",
    "@baloise/vue-axios": "^1.2.0",
    "@baloise/web-app-pipes-vue": "^2.0.5",
    "@baloise/web-app-utils": "^2.0.5",
    "@baloise/web-app-validators-vue": "^2.0.5",
    "ag-grid-community": "^26.2.0",
    "axios": "^0.21.4",
    "core-js": "^3.19.1",
    "vee-validate": "^4.5.6",
    "vue": "^3.2.22",
    "vue-class-component": "^8.0.0-beta.3",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@baloise/design-system-testing": "^2.0.0-beta.1",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-e2e-cypress": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-router": "^4.5.15",
    "@vue/cli-plugin-typescript": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.22",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "copyfiles": "^2.4.1",
    "cypress": "^6.9.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.20.0",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.2"
  },
  "resolutions": {
    "glob-parent": ">=5.1.2",
    "minimist": ">=0.2.1 <1.0.0 || >=1.2.3"
  }
}
